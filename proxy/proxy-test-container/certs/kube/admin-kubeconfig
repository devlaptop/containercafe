current-context: service-account-context
apiVersion: v1
kind: Config
contexts:
- context:
    cluster: local
    user: admin
    namespace: default
  name: service-account-context
clusters:
- name: local
  cluster:
    certificate-authority: ca.pem
    apiVersion: v1
    server: https://192.168.10.2:443
users:
- name: admin
  user:
    client-certificate: admin.pem
    client-key: admin-key.pem

